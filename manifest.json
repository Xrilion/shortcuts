{
  "manifest_version": 3,
  "name": "Shortcuts",
  "version": "4.0",
  "description": "A Fresh New Start",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "storage"
  ],
  "icons": {
    "16": "icons/16x16_icon.png",
    "32": "icons/32x32_icon.png",
    "48": "icons/48x48_icon.png",
    "128": "icons/128x128_icon.png"
  }
}
